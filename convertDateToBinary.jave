//Leet 3280. Convert Date to Binary
class convertDateToBinary {
    public String convertDateToBinary(String date) {
        String s=Integer.toBinaryString(Integer.parseInt(date.substring(0,4)))+'-'+Integer.toBinaryString(Integer.parseInt(date.substring(5,7)))+'-'+Integer.toBinaryString(Integer.parseInt(date.substring(8,10)));
        return s;
    }
}
